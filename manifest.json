{
	"manifest_version": 2,
	"name": "Elegant New Tab",
	"description": "A better New Tab",
	"version": "1.2.2",
	"chrome_url_overrides" : {
		"newtab": "/views/ent.html"
	},

	"permissions": [
        "http://*/*",
        "https://*/*",
        "chrome://favicon/",
        "topSites",
        "tabs",
        "geolocation",
        "unlimitedStorage",
        "notifications"
	]
}